{
  "$schema": "https://json.schemastore.org/prettierrc",
  "endOfLine": "lf",
  "bracketSameLine": true,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "quoteProps": "as-needed",
  "trailingComma": "all",
  "objectWrap": "preserve",
  "printWidth": 120,
  "proseWrap": "preserve",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false
}
